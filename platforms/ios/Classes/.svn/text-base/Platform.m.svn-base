@implementation Platform


@synthesize myPosition;
@synthesize myFrame;


-(id)initWithPosition:(Vertex3D)thePosition {
	if (self = [super init]) {
		myPosition = thePosition;
		myFrame = CGRectMake(myPosition.x - 15.0, myPosition.y, 30.0, 1.0);
	}
	return self;
}


-(void)draw {	
	GLfloat cubeVertices[] = {
		-myFrame.size.width / 2.0, 0.0, -1.0,            // top left (at rear)
        -myFrame.size.width / 2.0, 0.0, 1.0,             // bottom left (at front)
		myFrame.size.width / 2.0, 0.0, 1.0,              // bottom right (at front)
        myFrame.size.width / 2.0, 0.0, -1.0              // top right (at rear)
	};
	
	GLfloat cubeNormal[] = {
		0.0f, 0.0f, -1.0f
	};


	glPushMatrix();
	{	
		glTranslatef(myPosition.x, myPosition.y, myPosition.z);
		glColor4f(1.0, 0.0, 0.0, 0.75);
		
		glEnableClientState(GL_VERTEX_ARRAY);
		glVertexPointer(3, GL_FLOAT, 0, cubeVertices);
		
		glEnableClientState(GL_NORMAL_ARRAY);
		glNormalPointer(GL_FLOAT, 0, cubeNormal);
		
		glDrawArrays(GL_LINE_LOOP, 0, 4);
	}
	glPopMatrix();
}


@end
