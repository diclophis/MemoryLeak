<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "http://www.asahi-net.or.jp/~cs8k-cyu/bulletml/bulletml.dtd">

<bulletml type="horizontal"
          xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">

<action label="top">
  <repeat>
    <times>100</times>
    <action>
      <repeat>
        <times>2+$rank*1.5</times>
        <action>
         <fire>
          <bulletRef label="rfRkt"/>
         </fire>
         <wait>20</wait>
        </action>
      </repeat>
    </action>
    <wait>20</wait>
  </repeat>
</action>

<bullet label="rfRkt">
  <action>
    <repeat>
      <times>100</times>
      <action>
        <wait>20</wait>
        <fire>
          <direction type="sequence">13</direction>
          <bullet/>
        </fire>
      </action>
    </repeat>
  </action>
</bullet>

</bulletml>
